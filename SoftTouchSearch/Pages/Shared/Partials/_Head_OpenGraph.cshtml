@* head tags for Open Graph *@
<meta property="og:title" content="Soft Touch Search" />
<meta property="og:type" content="website" />
<meta property="og:image" content="/img/logo-fullscreen.jpg" />
@if (ViewData["Canonical"] != null)
{
    <meta property="og:url" content="@ViewData["Canonical"]" />
}
@* head tags for Twitter Cards *@
<meta name="twitter:card" content="summary_large_image">
<meta name="twitter:title" content="Soft Touch Search">
<meta name="twitter:description" content="Search the text of the web novel Soft Touch.">
<meta name="twitter:image" content="/img/logo-fullscreen.jpg">